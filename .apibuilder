code:
  apicollective:
    apibuilder-common:
      version: latest
      generators:
        ruby_client:
          target: src/apibuilder-cli
    apibuilder-api:
      version: latest
      generators:
        ruby_client:
          target: src/apibuilder-cli
          files: apicollective*.*
    apibuilder-generator:
      version: latest
      generators:
        ruby_client:
          target: src/apibuilder-cli
          files:
            - apicollective*.*
            - '*client.rb'
    apibuilder-spec:
      version: latest
      generators:
        ruby_client: src/apibuilder-cli
